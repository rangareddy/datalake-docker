	</property>
	<property>
		<name>fs.s3.secret.key</name>
		<value>password</value>
	</property>
	<property>
		<name>fs.s3.aws.credentials.provider</name>
		<value>org.apache.hadoop.fs.s3a.SimpleAWSCredentialsProvider</value>
